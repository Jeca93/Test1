var i,j, k;
var n=5
for (i=n-1;i>1; i--){
    for (j=n; j>i; j--)
    console.log("*");
    for(k=1; k<i*2; k++)
    console.log("&nbsp;&nbsp");
    console.log("<br>")
}
for (i=1; i<=n;i++){
    for (j=i; j<n; j++)
    console.log("*");
    for (k=1;k<i*2;k++)
    console.log("&nbsp;&nbsp");
    console.log("<br>")
}
